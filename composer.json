{
    "name": "w3btech/w3btlaravelkit",
    "version": "1.0.2",
    "description": "Paquete de Automatizacion Inicial de Configuracion de Proyectos Laravel PHP.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "w3btlaravelkit\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Francisco J. Morales",
            "email": "contacto@w3btech.com",
            "role": "Desarrollador"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "w3btlaravelkit\\Providers\\W3BTLaravelKitServiceProvider"
            ]
        }
    },
    "require": {
        "php": ">=8.1",
        "illuminate/console": "*"
    },
    "require-dev": {
        "laravel/framework": "^v12.0",
        "laravel/pint": "^1.16",
        "illuminate/support": "*"
    },
    "conflict": {
    "laravel/framework": "<12.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
